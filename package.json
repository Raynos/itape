{
  "name": "itape",
  "version": "1.0.1",
  "description": "Interactive tape test runner",
  "keywords": [],
  "author": "Raynos <raynos2@gmail.com>",
  "repository": "git://github.com/Raynos/itape.git",
  "main": "index",
  "homepage": "https://github.com/Raynos/itape",
  "bugs": {
    "url": "https://github.com/Raynos/itape/issues",
    "email": "raynos2@gmail.com"
  },
  "bin": {
    "itape": "./index.js"
  },
  "contributors": [
    {
      "name": "Raynos"
    }
  ],
  "dependencies": {
    "home-path": "^0.1.1",
    "minimist": "^1.1.0",
    "mkdirp": "^0.5.0",
    "parents": "^1.0.0",
    "process": "^0.10.0",
    "readable-stream": "^1.0.33",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.10.0",
    "istanbul": "^0.2.7",
    "jshint": "^2.5.0",
    "pre-commit": "0.0.5",
    "run-browser": "^1.3.0",
    "tap-spec": "^0.1.8",
    "tape": "^3.0.3"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/Raynos/itape/raw/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "npm run jshint -s && NODE_ENV=test node test/index.js | tap-spec",
    "unit-test": "NODE_ENV=test node test/index.js | tap-spec",
    "jshint-pre-commit": "jshint --verbose $(git diff --cached --name-only | grep '\\.js$')",
    "jshint": "jshint --verbose .",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0)",
    "phantom": "run-browser test/index.js -b",
    "browser": "run-browser test/index.js"
  },
  "engine": {
    "node": ">= 0.8.x"
  },
  "pre-commit": [
    "jshint-pre-commit",
    "unit-test"
  ],
  "ngen-version": "4.0.3"
}
